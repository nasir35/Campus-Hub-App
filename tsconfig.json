{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "backend/server.js",
    "nativewind-env.d.ts"
, "app/posts/[id].jsx", "app/followers/[id].jsx", "app/followings/[id].jsx"  ]
}